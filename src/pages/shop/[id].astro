---
import Layout from "../../layouts/Layout.astro";
import SectionProduct from "../../sections/SectionProduct.astro";
import { productsData } from "../../utils/data";

export function getStaticPaths() {
  return productsData.map((product) => ({
    params: { id: product.url },
    props: { product },
  }))
}

const { product } = Astro.props;
---

<Layout title={`brillaire  /${product.url}`}>
  <SectionProduct productData={product}/>
</Layout>

